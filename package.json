  "description": "a admin system build for react",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --inline",
    "build": "npm run build-mac",
    "build-mac": "rm -rf ./dist && set NODE_ENV=production && webpack -p",
    "build-win": "rd/s/q ./dist && set NODE_ENV=production && webpack -p",
    "view-mac": "npm run build-mac && node ./service/app.js",
    "view-win": "npm run build-win && node ./service/app.js",
    "analyz": "set npm_config_report=true && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react"
  ],
  "browerslist": [
